<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<Button mnemonicParsing="false" prefHeight="50.0" prefWidth="205.0" style="-fx-background-color: transparent;" textFill="#908c8c" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <font>
      <Font name="System Bold" size="15.0" />
   </font>
   <graphic>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: transparent;">
         <children>
            <ImageView fx:id="tabImage" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="15.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <Label fx:id="tabText" textAlignment="CENTER">
               <font>
                  <Font name="Helvetica Neue" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </graphic>
</Button>
